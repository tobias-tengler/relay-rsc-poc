{
  "name": "relay-rsc-poc",
  "private": true,
  "version": "0.0.0",
  "description": "A POC to show how to use Relay with RSC",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm run -r build",
    "watch": "pnpm run --filter=relay-rsc watch",
    "dev": "pnpm run --filter=example dev",
    "start": "pnpm run --filter=example start",
    "relay": "pnpm run --filter=example relay"
  },
  "engines": {
    "node": ">=20",
    "pnpm": "8"
  },
  "packageManager": "pnpm@8.6.1",
  "author": "Tobias Tengler & Jan Nicklas",
  "license": "MIT",
  "devDependencies": {}
}
